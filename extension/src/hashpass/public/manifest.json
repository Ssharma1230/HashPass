{
    "manifest_version": 3,
    "name": "HashPass",
    "version": "1.0",
    "description": "A Chrome extension with React and Next.js",
    "action": {
      "default_popup": "popup/index.html"
    },
    "options_page": "login/index.html",
    "web_accessible_resources": [
        {
          "resources": ["dashboard/index.html"],
          "matches": ["<all_urls>"]
        }
    ]
  }
  