{
    "manifest_version": 3,
    "name": "HashPass",
    "version": "1.0",
    "description": "A Chrome extension with React and Next.js",
    "permissions": ["storage", "scipting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
}
  